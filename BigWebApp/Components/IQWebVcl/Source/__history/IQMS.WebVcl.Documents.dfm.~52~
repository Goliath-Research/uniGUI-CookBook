  object Qry_IntDocs: TFDQuery
    BeforeInsert = DoBeforeInsert
    BeforePost = DoBeforePost
    OnCalcFields = DoCalcFields
    OnNewRecord = DoNewRecord
    ConnectionName = 'IQFD'
    FetchOptions.AssignedValues = [evItems]
    FetchOptions.Items = [fiBlobs, fiDetails]
    UpdateOptions.AssignedValues = [uvUpdateChngFields, uvUpdateMode, uvLockMode, uvRefreshMode, uvCheckRequired, uvCheckReadOnly, uvCheckUpdatable]
    UpdateOptions.CheckRequired = False
    UpdateOptions.CheckReadOnly = False
    UpdateOptions.CheckUpdatable = False
    UpdateOptions.KeyFields = 'ID'
    UpdateObject = UpdateSQL_IntDocs
    SQL.Strings = (
      'select'
      '  lnk.ID, '
      '  lnk.SOURCE_ID,'
      '  lnk.IQ_DOCS_ID,'
      '  lnk.SEQ, '
      '  lnk.PRINT_WITH,'
      '  doc.DOC_CLASS, '
      '  doc.DESCRIP,'
      '  doc.DOC_BLOB,'
      '  doc.USERID,'
      '  doc.TIMESTAMP'
      'from'
      '  internal_doc_used lnk'
      '    left outer join IQ_DOCS doc'
      '      on lnk.IQ_DOCS_ID = doc.ID'
      'where'
      '  lnk.SOURCE = :SOURCE and '
      '  lnk.SOURCE_ID = :SOURCE_ID'
      'order by lnk.SEQ')
    Left = 104
    Top = 152
    ParamData = <
      item
        Name = 'SOURCE'
        DataType = ftString
        ParamType = ptInput
        Size = 50
        Value = Null
      end
      item
        Name = 'SOURCE_ID'
        DataType = ftFloat
        ParamType = ptInput
        Value = Null
      end>
    object Qry_IntDocsID: TBCDField
      FieldName = 'ID'
      Origin = 'ID'
      Precision = 15
      Size = 0
    end
    object Qry_IntDocsSOURCE_ID: TBCDField
      FieldName = 'SOURCE_ID'
      Origin = 'SOURCE_ID'
      Required = True
      Precision = 15
      Size = 0
    end
    object Qry_IntDocsIQ_DOCS_ID: TBCDField
      FieldName = 'IQ_DOCS_ID'
      Origin = 'IQ_DOCS_ID'
      Required = True
      Precision = 15
      Size = 0
    end
    object Qry_IntDocsSEQ: TBCDField
      FieldName = 'SEQ'
      Origin = 'SEQ'
      Precision = 8
      Size = 0
    end
    object Qry_IntDocsPRINT_WITH: TStringField
      FieldName = 'PRINT_WITH'
      Origin = 'PRINT_WITH'
      Size = 25
    end
    object Qry_IntDocsDOC_CLASS: TStringField
      FieldName = 'DOC_CLASS'
      Origin = 'DOC_CLASS'
      FixedChar = True
      Size = 2
    end
    object Qry_IntDocsDESCRIP: TStringField
      FieldName = 'DESCRIP'
      Origin = 'DESCRIP'
      FixedChar = True
      Size = 30
    end
    object Qry_IntDocsDOC_BLOB: TBlobField
      FieldName = 'DOC_BLOB'
      Origin = 'DOC_BLOB'
      BlobType = ftOraBlob
    end
    object Qry_IntDocsUSERID: TStringField
      FieldName = 'USERID'
      Origin = 'USERID'
      Size = 35
    end
    object Qry_IntDocsTIMESTAMP: TDateTimeField
      FieldName = 'TIMESTAMP'
      Origin = 'TIMESTAMP'
    end
    object Qry_IntDocsPrintWithToShow: TStringField
      FieldKind = fkCalculated
      FieldName = 'PrintWithToShow'
      Size = 100
      Calculated = True
    end
  end
