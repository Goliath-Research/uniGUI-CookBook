      FieldKind = fkCalculated
      FieldName = 'New_Value_Disp'
      Size = 4000
      Calculated = True
    end
    object wwQryTR_ValueDATA_TYPE: TStringField
      FieldName = 'DATA_TYPE'
      Origin = 'IQ.USER_TAB_COLUMNS.DATA_TYPE'
