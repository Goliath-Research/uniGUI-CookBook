  object QryUniqueItems: TFDQuery
    BeforeOpen = AssignParamBeforeOpen
    OnCalcFields = QryUniqueItemsCalcFields
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate]
    UpdateOptions.EnableDelete = False
    UpdateOptions.EnableInsert = False
    UpdateOptions.EnableUpdate = False
    SQL.Strings = (
      'select distinct'
      '       t.arinvt_id,'
      '       t.class,'
      '       t.itemno,'
      '       t.rev,'
      '       t.descrip,'
      '       t.unit, '
      '       t.eplant_id'
      '  from translog t, arinvt a'
      ' where t.trans_in_out = '#39'IN'#39
      '   and t.trans_date between :date1 and :date2'
      '   and t.arinvt_id is not NULL'
      '   and misc.eplant_filter( t.eplant_id ) = 1'
      ''
      '   and t.arinvt_id = a.id'
      '   and a.standard_id is not null'
      '   and NVL(t.parent_arinvt_id, 0) = 0'
      ' order by t.itemno')
    Left = 24
    Top = 80
    ParamData = <
      item
        Name = 'date1'
        DataType = ftDateTime
        ParamType = ptInput
      end
      item
        Name = 'date2'
        DataType = ftDateTime
        ParamType = ptInput
      end>
