  inherited wwQryLocations: TFDQuery
    SQL.Strings = (
      '  SELECT loc.loc_desc,'
      '         d.name AS division_name,'
      '         loc.division_id,'
      '         loc.descrip,'
      '         loc.eplant_id,'
      '         loc.vmi,'
      '         loc.non_conform,'
      '         loc.ship_default,'
      '         loc.transit,'
      '         loc.id,'
      '         loc.building'
      '    FROM locations loc, division d'
      '   WHERE loc.division_id = d.id(+) AND'
      '         NVL(loc.pk_hide, '#39'N'#39') <> '#39'Y'#39' AND'
      '          misc.eplant_filter(loc.eplant_id) = 1'
      'ORDER BY loc_desc')
