  object PkLoc: TIQWebHpick
    BeforeOpen = PkLocBeforeOpen
    Connection = UniMainModule.FDConnection1
    SQL.Strings = (
      'select l.id as id,'
      '       l.loc_desc as location,'
      '       l.start_row as path,'
      '       l.descrip as description,'
      '       '#39'['#39' || e.ID || '#39']  '#39' || e.name as eplant_name,'
      '       d.name as division,'
      '       d.id as division_id,'
      '       l.pk_hide,         '
      '       l.building'
      '  from locations l,'
      '       eplant e,'
      '       division d'
      ' where l.eplant_id = e.id(+)'
      '   and l.division_id = d.id(+)'
      '   and misc.eplant_filter(l.eplant_id) = 1'
      '   and (:division_id = 0 '
      '        or'
      '        :division_id = -1 and l.division_id is null'
      '        or'
      '        :division_id > 0 and l.division_id = :division_id )'
      ''
      ''
      ''
      ''
      ' ')
    Title = 'Select from Pick List'
    Touchscreen = False
    ForceShowWaitForPrompt = False
    Left = 311
    Top = 144
    ParamData = <
      item
        Name = 'division_id'
        DataType = ftBCD
        ParamType = ptInput
      end>
