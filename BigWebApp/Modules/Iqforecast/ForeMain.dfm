      '       v.ext_price,'
      '       v.mm_dd_yy,'
      '       v.qty,'
      '       v.note1,'
      '       v.ship_to_id,'
      '       a.descrip2,'
      '       v.division'
      'from   arinvt a,'
      '       fr_group g,'
      '       arcusto c,'
      '       ( '
      '          select i.arinvt_id,'
      '                t.arcusto_id,'
      '                NVL(d.price,0) as price,'
      '                NVL( d.qty * d.price, 0) as ext_price,'
      '                d.req_date as mm_dd_yy,'
      '                NVL(d.qty,0) as qty,'
      '                d.note1,'
      '                d.ship_to_id,'
      '                w.name as division'
      '          from fore_item i,'
      '                fore_cust t,'
      '                fore_dtl d,'
      '                division w'
      '          where '
      '                 i.fore_head_id = :fore_head_id'
      '             and i.id = t.fore_item_id(+)'
      '             and t.id = d.fore_cust_id(+)'
      '             and w.id(+) = d.division_id'
      
        '             and (t.arcusto_id = :aarcusto_id or :all_customers ' +
        '= 1)'
      
        '             and (d.ship_to_id = :aship_to_id or :any_ship_to = ' +
        '1)'
      
        '             and (:afrom_date <= d.req_date and d.req_date <= :a' +
        'to_date)'
      '          UNION'
      '          select i.arinvt_id,'
      '                t.arcusto_id,'
      '                0 as price,'
      '                0 as ext_price,'
      '                to_date(null) as mm_dd_yy,'
      '                0 as qty,'
      '                '#39#39' as note1,'
      '                d.ship_to_id,'
      '                '#39#39' as division'
      '          from fore_item i,'
      '                fore_cust t,'
      '                fore_dtl d'
      '         where '
      '               i.fore_head_id = :fore_head_id'
      '            and i.id = t.fore_item_id(+)'
      '            and t.id = d.fore_cust_id(+)'
      '            and d.id is NULL'
      
        '            and (t.arcusto_id = :aarcusto_id or :all_customers =' +
        ' 1)'
      
        '            and (d.ship_to_id = :aship_to_id or :any_ship_to = 1' +
        ')'
      '       ) v'
      'where a.id = v.arinvt_id(+)'
      
        '   and (a.standard_id is not NULL or NVL(a.fr_include, '#39'N'#39') = '#39'Y' +
        #39')'
      '   and v.arcusto_id = c.id(+)'
      '   and a.fr_group_id = g.id(+)'
      '   and (:any_group = 1 or g.id = :group_id)'
      '   and misc.eplant_filter( a.eplant_id ) = 1'
      '   and (c.id = :aarcusto_id or :all_customers = 1)'
      '   and nvl(rtrim(substr(a.class, 1, 1)),'#39'*'#39') <> '#39'~'#39
      '   and (:exclude_non_salable = '#39'N'#39
      '        or'
      
        '        :exclude_non_salable = '#39'Y'#39' and nvl(a.non_salable, '#39'N'#39') <' +
        '> '#39'Y'#39')'
      'UNION'
      ''
      'select a.itemno,'
      '       a.id as arinvt_id,'
      '       a.descrip,'
      '       a.rev,'
      '       c.company,'
      '       a.arcusto_id,'
      '       c.custno,'
      '       0 as price,'
      '       0 as ext_price,'
      '       to_date(null) as mm_dd_yy,'
      '       0 as qty,'
      '       '#39#39' as note1,'
      '       to_number(null) as ship_to_id,'
      '       a.descrip2,'
      '       '#39#39' as division'
      'from   arinvt a,'
      '       fr_group g,'
      '       arcusto c'
      'where a.standard_id is not NULL'
      '   and a.arcusto_id = c.id'
      '   and a.fr_group_id = g.id(+)'
      '   and (:any_group = 1 or g.id = :group_id)'
      '   and misc.eplant_filter( a.eplant_id ) = 1'
      '   and (c.id = :aarcusto_id or :all_customers = 1)'
      '   and (:any_ship_to = 1)'
      '   and nvl(rtrim(substr(a.class, 1, 1)),'#39'*'#39') <> '#39'~'#39
      '   and (:exclude_non_salable = '#39'N'#39
      '        or'
      
        '        :exclude_non_salable = '#39'Y'#39' and nvl(a.non_salable, '#39'N'#39') <' +
        '> '#39'Y'#39')'
      '                     '
      'UNION'
      ''
      'select distinct a.itemno,'
      '       a.id as arinvt_id,'
      '       a.descrip,'
      '       a.rev,'
      '       c.company,'
      '       k.arcusto_id,'
      '       c.custno,'
      '       0 as price,'
      '       0 as ext_price,'
      '       to_date(null) as mm_dd_yy,'
      '       0 as qty,'
      '       '#39#39' as note1,'
      '       to_number(null) as ship_to_id,'
      '       a.descrip2,'
      '       '#39#39' as division'
      'from   arinvt a,'
      '       fr_group g,'
      '       arcusto c,'
      '       aka k'
      ' where a.id = k.arinvt_id'
      '   and a.standard_id is not NULL'
      '   and k.arcusto_id = c.id'
      '   and a.fr_group_id = g.id(+)'
      '   and (:any_group = 1 or g.id = :group_id)'
      '   and misc.eplant_filter( a.eplant_id ) = 1'
      '   and (c.id = :aarcusto_id or :all_customers = 1)'
      '   and (:any_ship_to = 1)'
      '   and nvl(rtrim(substr(a.class, 1, 1)),'#39'*'#39') <> '#39'~'#39
      '   and (:exclude_non_salable = '#39'N'#39
      '        or'
      
        '        :exclude_non_salable = '#39'Y'#39' and nvl(a.non_salable, '#39'N'#39') <' +
        '> '#39'Y'#39')')
