  object QryOrders: TFDQuery
    Connection = UniMainModule.FDConnection1
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate]
    UpdateOptions.EnableDelete = False
    UpdateOptions.EnableInsert = False
    UpdateOptions.EnableUpdate = False
    SQL.Strings = (
      'select r.id as releases_id,'
      '       d.id as ord_detail_id,'
      '       o.id as orders_id,'
      '       o.orderno'
      '  from orders o, '
      '       ord_detail d,'
      '       releases r'
      ' where o.id = d.orders_id'
      '   and r.ord_detail_id(+) = d.id'
      '   and o.type = '#39'AUTO-MRP'#39
      '   and d.arinvt_id = :id'
      ''
      ' ')
    Left = 200
    Top = 269
    ParamData = <
      item
        Name = 'id'
        DataType = ftBCD
      end>
  end
