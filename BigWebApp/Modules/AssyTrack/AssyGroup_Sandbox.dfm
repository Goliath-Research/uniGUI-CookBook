  object PkAssyGroup: TIQWebHpick
    BeforeOpen = PkAssyGroupBeforeOpen
    Connection = UniMainModule.FDConnection1
    SQL.Strings = (
      'select ag.id as id,'
      '       ag.descrip as descrip,'
      '       ag.planned_cntr_type_id as planned_cntr_type_id,'
      '       ag.locations_id as locations_id,'
      '       ag.work_center_id as work_center_id,'
      '       ag.start_date as start_date,'
      '       ct.name as cntr_type_name,'
      '       loc.loc_desc as loc_desc,'
      '       wc.eqno as eqno'
      '  from assy_group ag,'
      '       cntr_type ct,'
      '       locations loc,'
      '       work_center wc'
      ' where ag.planned_cntr_type_id = ct.id(+)'
      '   and ag.locations_id = loc.id(+)'
      '   and ag.work_center_id = wc.id(+)'
      '   and nvl(ag.assy_run, '#39'N'#39') <> '#39'Y'#39
      '   and ag.cntr_type_id is null'
      '   and ag.planned_cntr_type_id = :cntr_type_id'
      '         '
      '')
    Title = 'Select from Pick List'
    Touchscreen = False
    ForceShowWaitForPrompt = False
    Left = 75
    Top = 180
