  object QryWorkorders: TFDQuery
    AutoCalcFields = False
    MasterSource = SrcAssyGroup
    MasterFields = 'ID'
    DetailFields = 'ID'
    Connection = UniMainModule.FDConnection1
    FetchOptions.AssignedValues = [evItems, evCache]
    FetchOptions.Cache = [fiBlobs, fiMeta]
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate]
    UpdateOptions.EnableDelete = False
    UpdateOptions.EnableInsert = False
    UpdateOptions.EnableUpdate = False
    UpdateOptions.KeyFields = 'PTORDER_REL_ID'
    SQL.Strings = (
      'select wo.id as id,'
      '       wo.groupid as groupid,'
      '       wo.mfgno as mfgno,'
      '       wo.bucket as bucket,'
      '       b.itemno as itemno,'
      '       b.descrip as descrip,'
      
        '       substrB(iqeval.substrW63Max( assy1_misc.get_grouping_sndo' +
        'p_cntr_type( b.partno_id ), 1, 20), 1, 20) as cntr_type,'
      ''
      '       /*wo.prodhrs as prodhrs*/'
      
        '       assy1_misc.get_grouping_sndop_progress( wo.id, b.partno_i' +
        'd, b.id, '#39'HOURS_TO_GO'#39' ) as prodhrs,'
      '       '
      '       wo.setuphrs as setuphrs,'
      
        '       decode( RTrim(wo.lookup_mfgtype), '#39'EXTRUSION'#39', iqms.uom.i' +
        'qconvert( '#39'FT'#39', wo.uom, wo.cycles_req, -1, '#39'Y'#39' ), wo.cycles_req ' +
        ') as cycles_req,'
      '       wo.start_time as start_time,'
      '       wo.company as company,'
      '       b.rel_date as ship_date,'
      '       p.promise_date as promise_date,'
      '       p.request_date as request_date,'
      '       decode(wo.firm, '#39'Y'#39', '#39'FIRM'#39', '#39#39') as work_order_type,'
      '       wo.origin as origin,'
      '       RTrim(wo.mfg_type) as mfg_type,'
      '       wo.lookup_mfgtype as lookup_mfgtype,'
      '       wo.mfgcell as mfgcell,'
      '       wo.eplant_id as eplant_id,'
      '       b.rev as rev,'
      '       b.descrip2 as descrip2,'
      '       wo.standard_id as standard_id,'
      '       p.id as ptallocate_id,'
      
        '       workorder_misc.is_item_consumed( b.arinvt_id ) as is_cons' +
        'umed,'
      '       wo.assy_group_id,'
      '       b.ptorder_rel_id,'
      '       b.group_code'
      '  from             '
      '       v_workorder_standard wo,'
      '       v_ptorder_partno b,'
      '       ptallocate p'
      ' where wo.id = b.workorder_id'
      '   and p.id(+) = b.ptallocate_id'
      '   and misc.eplant_filter( wo.eplant_id ) = 1'
      '   and wo.assy_group_id = :id')
    Left = 83
    Top = 120
