  object UpdateSQL1: TFDUpdateSQL
    ConnectionName = 'IQFD'
    InsertSQL.Strings = (
      'insert into exp_type'
      
        '  (ID, DESCRIPTION, GLACCT_ID_EXPENSE, REF_CODE_ID, CUSER1, CUSE' +
        'R2, '
      'CUSER3, '
      
        '   NUSER1, NUSER2, NUSER3, BILLABLE_RATE, BILLABLE_UOM, VECHICLE' +
        ')'
      'values'
      
        '  (:ID, :DESCRIPTION, :GLACCT_ID_EXPENSE, :REF_CODE_ID, :CUSER1,' +
        ' '
      ':CUSER2, '
      
        '   :CUSER3, :NUSER1, :NUSER2, :NUSER3, :BILLABLE_RATE, :BILLABLE' +
        '_UOM, '
      ':VECHICLE)')
    ModifySQL.Strings = (
      'update exp_type'
      'set'
      '  ID = :ID,'
      '  DESCRIPTION = :DESCRIPTION,'
      '  GLACCT_ID_EXPENSE = :GLACCT_ID_EXPENSE,'
      '  REF_CODE_ID = :REF_CODE_ID,'
      '  CUSER1 = :CUSER1,'
      '  CUSER2 = :CUSER2,'
      '  CUSER3 = :CUSER3,'
      '  NUSER1 = :NUSER1,'
      '  NUSER2 = :NUSER2,'
      '  NUSER3 = :NUSER3,'
      '  BILLABLE_RATE = :BILLABLE_RATE,'
      '  BILLABLE_UOM = :BILLABLE_UOM,'
      '  VECHICLE = :VECHICLE'
      'where'
      '  ID = :OLD_ID')
    DeleteSQL.Strings = (
      'delete from exp_type'
      'where'
      '  ID = :OLD_ID')
