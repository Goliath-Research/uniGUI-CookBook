      '                    v.country,'
      '                    v.zip,'
      '                    r.attn,'
      '                    r.addr1,'
      '                    r.addr2,'
      '                    r.addr3,'
      '                    r.city,'
      '                    r.state,'
      '                    r.zip,'
      '                    r.country,'
      '                    a.custno,'
      '                    a.company,'
      '                    a.addr1,'
      '                    a.addr2,'
      '                    a.addr3,'
      '                    a.city,'
      '                    a.state,'
      '                    a.country,'
      '                    a.zip,'
      '                    :cGlacct,'
