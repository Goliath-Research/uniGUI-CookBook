  object QryAllInv: TFDQuery
    BeforeOpen = QryAllInvBeforeOpen
    Filtered = True
    CachedUpdates = True
    OnUpdateRecord = QryAllInvUpdateRecord
    SQL.Strings = (
      'select v.id,'
      '       v.on_hold, '
      '       v.vendor_id, '
      '       v.invoice_no, '
      '       v.invoice_amount, '
      '       v.terms_id, '
      '       v.invoice_date, '
      '       v.due_date, '
      '       v.discount_days, '
      '       v.discount, '
      '       v.amount_paid, '
      '       v.discount_taken, '
      '       v.amount_to_pay,'
      
        '       v.amount_to_pay - decode(Sign(v.disc_date - to_date(:nDat' +
        'e, '#39'MM/DD/RRRR'#39')), -1, 0, v.current_discount) as actual_amt_to_p' +
        'ay,'
      
        '       decode(Sign(v.disc_date - to_date(:nDate, '#39'MM/DD/RRRR'#39')),' +
        ' -1, 0, v.current_discount) as actual_discount,'
      '       --0 as actual_amt_to_pay,'
      '       --0 as actual_discount,'
      '       v.Current_Discount,  '
      '       v.Disc_Date,'
      '       v.remit_to_id,'
      '       v.remit_to_attn,'
      '       v.curr_code,'
      '       v.currency_descrip,'
      '       v.acct_id_apdisc,'
      '       v.currency_id,'
      '       n.Company,'
      '       v.eplant_id         '
      '  from v_apinvoice_cheque v, '
      '       v_vendor n '
      ' where v.vendor_id = n.id'
      '   and NVL(v.amount_to_pay, 0) <> 0'
      ' '
      ' '
      ' '
      ' '
      ' '
      ' ')
    Left = 46
    Top = 97
    ParamData = <
      item
        Name = 'nDate'
        DataType = ftDateTime
        ParamType = ptInput
      end>
