  object QryARTaxCode: TFDQuery
    Connection = UniMainModule.FDConnection1
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate]
    UpdateOptions.EnableDelete = False
    UpdateOptions.EnableInsert = False
    UpdateOptions.EnableUpdate = False
    SQL.Strings = (
      'select e.descrip,'
      '       e.tax_rate,'
      '       sum(nvl(e.tax_amount, 0)) as amount,'
      '       d.eplant_id'
      '  from arprepost_tax e,'
      '          arprepost_detail d'
      ' where e.arprepost_detail_id = :Arprepostdetailid'
      '  and e.arprepost_detail_id(+) = d.id'
      'group by e.descrip, e.tax_rate, d.eplant_id'
      '   ')
    Left = 40
    Top = 48
    ParamData = <
      item
        Name = 'Arprepostdetailid'
        DataType = ftBCD
        ParamType = ptInput
      end>
    object QryARTaxCodeDESCRIP: TStringField
      DisplayLabel = 'Tax'
      DisplayWidth = 25
      FieldName = 'DESCRIP'
      Origin = 'e.descrip'
      Size = 60
    end
    object QryARTaxCodeTAX_RATE: TFMTBCDField
      DisplayLabel = 'Tax Rate'
      DisplayWidth = 10
      FieldName = 'TAX_RATE'
      Origin = 'e.tax_rate'
      Size = 6
    end
    object QryARTaxCodeAMOUNT: TFMTBCDField
      DisplayLabel = 'Amount'
      DisplayWidth = 10
      FieldName = 'AMOUNT'
      Origin = 'sum(nvl(e.tax_amount, 0))'
      currency = True
      Size = 38
    end
    object QryARTaxCodeEPLANT_ID: TBCDField
      FieldName = 'EPLANT_ID'
      Origin = 'd.eplant_id'
      Size = 0
    end
  end
  object QryARAllTaxes: TFDQuery
    Connection = UniMainModule.FDConnection1
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate]
    UpdateOptions.EnableDelete = False
    UpdateOptions.EnableInsert = False
    UpdateOptions.EnableUpdate = False
    SQL.Strings = (
      'select e.descrip,'
      '       e.tax_rate,'
      '       sum(nvl(e.tax_amount, 0)) as amount,'
      '       d.eplant_id'
      '  from arprepost_tax e,'
      '       arprepost_detail d'
      ' where e.arprepost_detail_id(+) = d.id'
      '   and d.arprepost_id = :ArpreposId'
      'group by e.descrip, e.tax_rate, d.eplant_id')
    Left = 72
    Top = 48
    ParamData = <
