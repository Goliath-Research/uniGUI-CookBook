  object QryARPostTaxCode: TFDQuery
    Connection = UniMainModule.FDConnection1
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate]
    UpdateOptions.EnableDelete = False
    UpdateOptions.EnableInsert = False
    UpdateOptions.EnableUpdate = False
    SQL.Strings = (
      'select e.tax_elements_descrip as descrip,'
      '       e.tax_elements_tax_rate as tax_rate,'
      
        '--       Round(Round(NVL(a.invoice_qty, 0) * NVL(a.unit_price, 0' +
        '), 2) * NVL(e.tax_elements_tax_rate, 0)/100, 2) as amount'
      
        '--       Round(NVL(a.invoice_qty, 0) * NVL(a.unit_price, 0) * NV' +
        'L(e.tax_elements_tax_rate, 0)/100, 3) as amount'
      '       Round(e.tax_amount, 3) as amount'
      '  from arinvoice_detail_taxes e,                    '
      '       arinvoice_detail a'
      ' where e.arinvoice_detail_id = a.id'
      '   and a.id = :Arinvoicedetailid'
      '   ')
    Left = 40
    Top = 80
    ParamData = <
      item
        Name = 'Arinvoicedetailid'
        DataType = ftBCD
        ParamType = ptInput
      end>
    object QryARPostTaxCodeDESCRIP: TStringField
      DisplayLabel = 'Tax'
      FieldName = 'DESCRIP'
      Origin = 'e.tax_elements_descrip'
      Size = 60
    end
    object QryARPostTaxCodeTAX_RATE: TFMTBCDField
      DisplayLabel = 'Tax Rate'
      FieldName = 'TAX_RATE'
      Origin = 'e.tax_elements_tax_rate'
      Size = 6
    end
    object QryARPostTaxCodeAMOUNT: TFMTBCDField
      DisplayLabel = 'Amount'
      FieldName = 'AMOUNT'
      Origin = 'Round(e.tax_amount, 3)'
      currency = True
      Size = 38
    end
  end
  object QryARPostAllTaxes: TFDQuery
    Connection = UniMainModule.FDConnection1
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate]
    UpdateOptions.EnableDelete = False
    UpdateOptions.EnableInsert = False
    UpdateOptions.EnableUpdate = False
    SQL.Strings = (
      'select descrip, tax_rate, sum(amount) as amount from ('
      'select e.tax_elements_descrip as descrip,'
      '       e.tax_elements_tax_rate as tax_rate,'
      
        '--       Round(Round(NVL(a.invoice_qty, 0) * NVL(a.unit_price, 0' +
        '), 2) * NVL(e.tax_elements_tax_rate, 0)/100, 2) as amount'
      
        '--       Round(NVL(a.invoice_qty, 0) * NVL(a.unit_price, 0) * NV' +
        'L(e.tax_elements_tax_rate, 0)/100, 3) as amount'
      '       Round(e.tax_amount, 3) as amount'
      
        '  from arinvoice_detail_taxes e,                                ' +
        '         '
      '       arinvoice_detail a,'
      '       arinvoice h'
      ' where e.arinvoice_detail_id = a.id'
      '   and a.arinvoice_id = h.id'
      '   and h.id = :ArInvoiceId)'
      'group by descrip, tax_rate'
      '   ')
    Left = 72
    Top = 80
    ParamData = <
      item
        Name = 'ArInvoiceId'
        DataType = ftBCD
        ParamType = ptInput
      end>
    object QryARPostAllTaxesDESCRIP: TStringField
      DisplayLabel = 'Tax'
      FieldName = 'DESCRIP'
      Origin = 'descrip'
      Size = 60
    end
    object QryARPostAllTaxesTAX_RATE: TFMTBCDField
      DisplayLabel = 'Tax Rate'
      FieldName = 'TAX_RATE'
      Origin = 'tax_rate'
      Size = 6
    end
    object QryARPostAllTaxesAMOUNT: TFMTBCDField
      DisplayLabel = 'Amount'
      FieldName = 'AMOUNT'
      Origin = 'amount'
      currency = True
      Size = 38
    end
  end
