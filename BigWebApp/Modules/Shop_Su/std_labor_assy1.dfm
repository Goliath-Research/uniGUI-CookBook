  inherited QryStd_Labor: TFDQuery
    BeforeOpen = QryStd_LaborBeforeOpen
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate, uvRefreshDelete]
    SQL.Strings = (
      'select mfgtype,'
      '       mfgcell,'
      '       mfgcell_id,'
      '       shift,'
      '       emp_level_id,'
      
        '       Sum( decode( week_day, 1, labor_hrs, 0)) as Sun_Labor_Hrs' +
        ','
      
        '       Sum( decode( week_day, 2, labor_hrs, 0)) as Mon_Labor_Hrs' +
        ','
      
        '       Sum( decode( week_day, 3, labor_hrs, 0)) as Tue_Labor_Hrs' +
        ','
      
        '       Sum( decode( week_day, 4, labor_hrs, 0)) as Wed_Labor_Hrs' +
        ','
      
        '       Sum( decode( week_day, 5, labor_hrs, 0)) as Thu_Labor_Hrs' +
        ','
      
        '       Sum( decode( week_day, 6, labor_hrs, 0)) as Fri_Labor_Hrs' +
        ','
      '       Sum( decode( week_day, 7, labor_hrs, 0)) as Sat_Labor_Hrs'
      '  from std_labor'
      ' where ( :show_only_assy1 = 0 '
      '         or'
      
        '         :show_only_assy1 = 1 and mfg.is_similar_to_assy1(mfgtyp' +
        'e) = 1)'
      ' group by mfgtype,'
      '          mfgcell,'
      '          mfgcell_id,'
      '          shift,'
      '          emp_level_id'
      ' '
      ' ')
    Left = 47
    Top = 77
    ParamData = <
      item
        Name = 'SHOW_ONLY_ASSY1'
        DataType = ftFloat
        ParamType = ptInput
      end>
  end
  inherited QryMfgType: TFDQuery
    Active = False
    BeforeOpen = QryStd_LaborBeforeOpen
    SQL.Strings = (
      'select RTRim(mfgtype) as mfgtype'
      '  from mfgtype'
      ' where ( :show_only_assy1 = 0 '
      '         or'
      
        '         :show_only_assy1 = 1 and mfg.is_similar_to_assy1(mfgtyp' +
        'e) = 1)'
      ' order by mfgtype')
    ParamData = <
      item
        Name = 'SHOW_ONLY_ASSY1'
        DataType = ftFloat
        ParamType = ptInput
      end>
  end
