  inherited QryLoadMfgTypeCells: TFDQuery
    BeforeOpen = QryStd_LaborBeforeOpen
    SQL.Strings = (
      'select x.mfgtype,'
      '       x.mfgcell,'
      '       x.mfgcell_id,'
      '       p.id as mfgcell_param_id,'
      '       p.shifts as mfgcell_specific_shifts'
      '  from'
      '       ( select mfgtype as mfgtype,'
      '                NULL as mfgcell,'
      '                null as mfgcell_id'
      '           from mfgtype'
      '          where ( :show_only_assy1 = 0 '
      '                  or'
      
        '                  :show_only_assy1 = 1 and mfg.is_similar_to_ass' +
        'y1(mfgtype) = 1)'
      '         UNION '
      '         select mfgtype as mfgtype,'
      '                mfgcell as mfgcell,'
      '                id as mfgcell_id'
      '           from mfgcell'
      '          where ( :show_only_assy1 = 0 '
      '                  or'
      
        '                  :show_only_assy1 = 1 and mfg.is_similar_to_ass' +
        'y1(mfgtype) = 1)'
      '         MINUS'
      '         select mfgtype as mfgtype,'
      '                mfgcell as mfgcell,'
      '                mfgcell_id as mfgcell_id'
      '           from std_labor ) x,'
      '       mfgcell m,'
      '       mfgcell_param p'
      ' where '
      '       x.mfgcell_id = m.id(+)'
      '   and iqcalendar.get_mfgcell_param_id( m.id ) = p.id(+)'
      '')
    ParamData = <
      item
        Name = 'SHOW_ONLY_ASSY1'
        DataType = ftFloat
        ParamType = ptInput
      end>
  end
