  object QryPOInfo: TFDQuery
    BeforeOpen = QryPOInfoBeforeOpen
    OnCalcFields = QryPOInfoCalcFields
    Connection = UniMainModule.FDConnection1
    SQL.Strings = (
      'select pono,'
      
        '       decode( d.arinvt_id, NULL, misc_item, a.itemno ) as itemn' +
        'o,'
      '       d.seq,'
      '       -- r.qty_received,'
      
        '       round(UOM.IQConvert( d.unit, a.unit, r.qty_received, a.id' +
        ', '#39'Y'#39' ), 4)  as qty_received,'
      '       r.id as po_receipt_id,'
      '       d.unit as po_detail_uom, '
      '       a.unit as arinvt_uom,'
      '       d.arinvt_id,'
      
        '       (select container_qty from kb_control where arinvt_id = a' +
        '.id and rownum < 2) as kb_container_qty'
      '  from po_receipts r,'
      '       po_detail d,'
      '       arinvt a,'
