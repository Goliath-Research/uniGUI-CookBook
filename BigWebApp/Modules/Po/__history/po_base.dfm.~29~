  object QryEserverAct: TFDQuery
    AfterScroll = QryEserverActAfterScroll
    MasterFields = 'VENDOR_ID'
    Connection = UniMainModule.FDConnection1
    UpdateOptions.AssignedValues = [uvEDelete, uvEInsert, uvEUpdate]
    UpdateOptions.EnableDelete = False
    UpdateOptions.EnableInsert = False
    UpdateOptions.EnableUpdate = False
    SQL.Strings = (
      'select d.id as mon_act_detail_id,'
      '       h.id as action_id,'
      '       v.id as mon_vendor_edi_id,'
      '       d.act_mon_id,'
      '       d.type,'
      '       h.description,'
      '       v.vendor_id,'
      '       v.days_scope,'
      '       c.transaction_set_code,'
      '       c.unique_id,'
      '       v.auto_update_po,'
      '       v.update_blanket_qty'
      '  from mon_act_detail d,'
      '       mon_act h,'
      '       mon_vendor_edi v,'
      '       edi_partners_ts b, '
      '       transaction_set c'
      ' where d.act_mon_id = h.id'
      '   and d.type = '#39'VENDOR EDI'#39
      '   and v.mon_act_detail_id = d.id'
      '   and b.transaction_set_unique_id = c.unique_id(+)'
      '   and b.id(+) = edi_partners_ts_id   '
      '   and v.vendor_id = :VENDOR_ID')
    Left = 40
    Top = 104
    ParamData = <
      item
        Name = 'VENDOR_ID'
        DataType = ftBCD
        ParamType = ptInput
        Size = 8
      end>
